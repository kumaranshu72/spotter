{
  "name": "spotter",
  "version": "1.0.0",
  "description": "spotter is the backend service for tiyaka",
  "main": "server.js",
  "engines": {
    "node": "12.9.1",
    "npm": "6.14.7"
  },
  "scripts": {
    "lint": "./node_modules/.bin/tslint --project \".\"",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "serve": "npm run lint && npm run build && tsc && node lib/server.js",
    "compile": "tsc && node lib/server.js",
    "start": "nodemon -e ts --exec \"npm run lint && npm run build && npm run compile\"",
    "test": "mocha --exit --require babel-register-ts --require babel-polyfill test/**/*.spec.ts",
    "test-with-coverage": "nyc --reporter=html --reporter=text mocha --exit --require babel-register-ts --require babel-polyfill test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumaranshu72/spotter.git"
  },
  "keywords": [
    "spotter"
  ],
  "author": "Anshu <kumaranshu72@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumaranshu72/spotter/issues"
  },
  "homepage": "https://github.com/kumaranshu72/spotter#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.spec.ts",
      "coverage",
      "dist",
      "lib",
      "test",
      "src/server.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },

  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "joi": "14.3.1",
    "joi-date-extensions": "1.2.0",
    "lodash": "4.17.19",
    "moment": "2.27.0",
    "mongoose": "5.9.27",
    "morgan": "1.10.0",
    "nodemon": "2.0.4",
    "redis": "3.0.2",
    "swagger-jsdoc": "4.0.0",
    "swagger-ui-express": "4.1.4",
    "winston": "3.3.3",
    "winston-logrotate": "1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/node": "7.10.5",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/chai": "4.2.12",
    "@types/cors": "2.8.7",
    "@types/express": "4.17.7",
    "@types/helmet": "0.0.47",
    "@types/joi": "14.3.4",
    "@types/lodash": "4.14.158",
    "@types/mocha": "8.0.1",
    "@types/morgan": "1.9.1",
    "@types/sinon": "9.0.4",
    "@types/supertest": "2.0.10",
    "@types/swagger-jsdoc": "3.0.2",
    "@types/swagger-ui-express": "4.1.2",
    "@types/winston": "2.4.4",
    "babel-polyfill": "6.26.0",
    "babel-register-ts": "7.0.0",
    "chai": "4.2.0",
    "mocha": "8.1.0",
    "nyc": "15.1.0",
    "sinon": "9.0.2",
    "supertest": "4.0.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "lib/*"
    ]
  }
}
