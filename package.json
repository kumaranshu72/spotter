{
  "name": "spotter",
  "version": "1.0.0",
  "description": "spotter is the backend service for tiyaka",
  "main": "server.js",
  "engines": {
    "node": "14.7.0",
    "npm": "6.14.7"
  },
  "scripts": {
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "serve": "npm run build && tsc && node lib/index.js",
    "compile": "tsc && node lib/index.js",
    "start": "nodemon -e ts --exec \"npm run build && npm run compile\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumaranshu72/spotter.git"
  },
  "keywords": [
    "spotter"
  ],
  "author": "Anshu <kumaranshu72@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumaranshu72/spotter/issues"
  },
  "homepage": "https://github.com/kumaranshu72/spotter#readme",
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "mongoose": "5.9.27",
    "nodemon": "2.0.4",
    "redis": "3.0.2"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/node": "7.10.5",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "typescript": "3.9.7"
  }
}
